<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="default_txtsize">20.0dp</dimen>
    <color name="defult_txt_big_color">@color/black</color>
    <color name="defult_txt_small_color">@color/gray</color>

    <declare-styleable name="Setting_Custom_Word">
        <attr name="txt_size" format="dimension"/>
